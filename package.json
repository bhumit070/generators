{
	"name": "template-generators",
	"displayName": "template-generators",
	"description": "This will help you generate templates for your projects",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.68.0"
	},
	"icon": "images/generator.jpeg",
	"license": "MIT",
	"publisher": "BhoomitGanatra",
	"categories": [
		"Other",
		"Programming Languages",
		"Snippets"
	],
	"activationEvents": [
		"onCommand:template-generators.generate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "template-generators.generate",
				"title": "Generate"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.68.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.16.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.2"
	},
	"dependencies": {
		"axios": "^0.27.2"
	},
	"repository": {
		"url": "https://github.com/bhumit070/generators"
	}
}